{
  "extends": ["react-app", "plugin:react/recommended"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "warn",
    "no-unused-vars": "warn",
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error"
  }
}